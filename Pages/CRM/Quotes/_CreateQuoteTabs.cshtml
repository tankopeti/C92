@model Cloud9_2.Models.Quote

<ul class="nav nav-tabs" id="quoteTabs" role="tablist">
    <li class="nav-item" role="presentation">
        <button class="nav-link active" id="basic-tab" data-bs-toggle="tab" data-bs-target="#basic" type="button" role="tab">Basic Info</button>
    </li>
    <li class="nav-item" role="presentation">
        <button class="nav-link" id="items-tab" data-bs-toggle="tab" data-bs-target="#items" type="button" role="tab">Items</button>
    </li>
</ul>

<div class="tab-content mt-3">
    <div class="tab-pane fade show active" id="basic" role="tabpanel">
        @await Html.PartialAsync("_CreateQuoteBasicTab", Model)
    </div>
    <div class="tab-pane fade" id="items" role="tabpanel">
        @await Html.PartialAsync("_CreateQuoteItemsTab", Model)
    </div>
</div>